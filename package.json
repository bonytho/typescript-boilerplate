{
  "name": "bs-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Express TypeScript Boilerplate",
  "main": "app.js",
  "scripts": {
    "lint": "tslint .",
    "build": "tsc -p .",
    "clean": "rimraf dist/*",
    "start": "node dist/server.js",
    "build:all": "npm audit && npm run build ",
    "dev": "ts-node ./src/server.ts",
    "dev:debug": "nodemon --inspect src/server.ts",
    "dev:nodemon": "nodemon src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boneyt92/typescript-boilerplate.git"
  },
  "keywords": [
    "Express",
    "Typescript",
    "Boilerplate",
    "REST",
    "API"
  ],
  "author": "BonyThomas <bonyt92@gmail.com>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/boneyt92/typescript-boilerplate/issues"
  },
  "homepage": "https://github.com/boneyt92/typescript-boilerplate#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "helmet": "^4.1.1",
    "joi": "^17.2.1",
    "morgan": "^1.10.0",
    "typescript": "^4.0.3",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/helmet": "^4.0.0",
    "@types/morgan": "^1.9.1",
    "@types/uuid": "^8.3.0",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3"
  }
}
